{
  "name": "bfy/smart-delete",
  "description": "",
  "version": "0.1.0",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Bruno FERRY"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "doctrine/doctrine-bundle": "^2.3",
    "doctrine/orm": "^2.8",
    "symfony/console": "5.2.*",
    "symfony/dotenv": "5.2.*",
    "symfony/framework-bundle": "5.2.*",
    "symfony/flex": "^1.12",
    "symfony/yaml": "5.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^5.2",
    "symfony/maker-bundle": "^1.30"
  },
  "conflict": {
     "symfony/symfony": "*"
  } ,
  "autoload": {
    "psr-4": {
      "Bfy\\SmartDeleteBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bfy\\SmartDeleteBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "extra": {
    "public-dir": false
  }
}
